{
    "pipeline_mode": "vanilla",

    "experiment_name": "cifar10_torch_vgg16_pretrained",
    "generated_files_path": "./generated",
    "data_provider_name": "CIFAR10_DataProvider",

    "platform_constraints": {
        "cores": [
            {
                "max_axons": 50000,
                "max_neurons": 600,
                "count": 100
