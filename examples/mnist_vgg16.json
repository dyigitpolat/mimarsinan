{
    "pipeline_mode": "phased",

    "experiment_name": "mnist_vgg16_mapper",
    "generated_files_path": "./generated",
    "data_provider_path": "./src/mimarsinan/data_handling/data_providers/mnist32_data_provider.py",
    "data_provider_name": "MNIST32_DataProvider",

    "platform_constraints": {
        "cores": [
            {
                "max_axons": 5000,
                "max_neurons": 600,
                "count": 100
            },
            {
                "max_axons": 600,
                "max_neurons": 5000,
